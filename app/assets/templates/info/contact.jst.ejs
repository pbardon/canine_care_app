<!-- Page Content -->
<div class="container pageContent">
    <div id="newDogForm" class="well bs-component col-md-8 col-md-offset-2">
        <form class="form-horizontal">
                    <fieldset>
                          <legend>
                             Contact
                          </legend>
                          <div class="form-group">
                            <label for="contactName"
                                    class="col-lg-2 control-label">Name:</label>
                            <div class="col-lg-10">
                              <input type="text"
                                    class="form-control"
                                    id="contactName"
                                    name="name"
                                    placeholder="Name"
                                    value="">
                            </div>
                          </div>
                          <div class="form-group">
                            <label for="contactEmail"
                                    class="col-lg-2 control-label">Email:</label>
                            <div class="col-lg-10">
                              <input type="text"
                                    class="form-control"
                                    id="contactEmail"
                                    name="email"
                                    placeholder="Email"
                                    value="">
                            </div>
                          </div>

                          <div class="form-group">
                            <label for="contactMessage"
                                    class="col-lg-2 control-label">Message:</label>
                            <div class="col-lg-10">
                              <textarea class="form-control"
                                        rows="3"
                                        id="contactMessage"
                                        name="message">
                              </textarea>
                            </div>
                          </div>
                      </fieldset>
          </form>
      </div>
  </div>
